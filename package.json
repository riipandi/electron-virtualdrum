{
  "name": "electron-virtualdrum",
  "productName": "Virtual Drumming",
  "version": "1.0.0",
  "description": "Electron webview for virtualdrumming",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package:win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Virtual Drumming\"",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "installer:mac": "electron-installer-dmg ./release-builds/Electron\\ webview-darwin-x64/Electron\\ webview.app electron-webview --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns"
  },
  "author": "Aris Ripandi",
  "license": "MIT",
  "devDependencies": {
    "electron": "~7.0.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.1.0"
  },
  "dependencies": {}
}
